{
  "icons": {
    "48": "landmarks-48.png",
    "96": "landmarks-96.png"
  },

  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "18": "landmarks-18.png",
      "32": "landmarks-32.png",
      "36": "landmarks-36.png",
      "64": "landmarks-64.png"
    }
  },

  "applications": {
    "gecko": {
      "id": "ariaLandmarkNav@ibm.com",
      "strict_min_version": "53.0"
    }
  },

  "background": {
    "scripts": ["compatibility.js", "sendToActiveTab.js", "background.js"]
  },

  "permissions": [
    "storage", "webNavigation", "tabs"
  ]
}
