{
  "icons": {
    "16": "landmarks-16.png",
    "128": "landmarks-128.png"
  },

  "browser_action": {
    "default_icon": {
      "19": "landmarks-19.png",
      "38": "landmarks-38.png"
    }
  },

  "sidebar_action": {
    "default_title": "Landmarks",
    "default_panel": "popup.html",
    "default_icon": {
      "19": "landmarks-19.png",
      "38": "landmarks-38.png"
    }
  },

  "options_ui": {
    "chrome_style": true
  },

  "minimum_opera_version": "44",

  "background": {
    "scripts": ["compatibility.js", "injector.js"]
  },

  "content_scripts": [
    {
      "js": ["compatibility.js", "injector.js"]
    }
  ],

  "permissions": ["<all_urls>"]
}
