{
  "name": "landmarks",
  "version": "2.1.1",
  "private": true,
  "scripts": {
    "pre_build": "npm test",
    "_build": "node scripts/build.js",
    "post_build": "eslint build",
    "build:all": "npm run clean:all && npm run _build -- all",
    "build:chrome": "npm run clean:chrome && npm run _build -- chrome",
    "build:edge": "npm run clean:edge && npm run _build -- edge",
    "build:firefox": "npm run clean:firefox && npm run _build -- firefox",
    "build:opera": "npm run clean:opera && npm run _build -- opera",
    "clean:all": "npm run clean:firefox && npm run clean:chrome && npm run clean:opera && npm run clean:edge",
    "clean:cache": "rimraf build/png-cache",
    "clean:chrome": "rimraf build/chrome landmarks-*-chrome.zip",
    "clean:edge": "rimraf build/edge landmarks-*-edge.zip",
    "clean:firefox": "rimraf build/firefox landmarks-*-firefox.zip",
    "clean:opera": "rimraf build/opera landmarks-*-opera.zip",
    "precommit": "npm run build:all",
    "start:firefox": "web-ext run --source-dir build/firefox/",
    "pretest": "eslint .",
    "test": "node test/test.js"
  },
  "devDependencies": {
    "addons-linter": "~0.32",
    "archiver": "~2.1",
    "chalk": "~2.3",
    "deepmerge": "~2.0",
    "eslint": "~4.15",
    "fs-extra": "~5.0",
    "husky": "~0.14",
    "is-mergeable-object": "~1.1",
    "jsdom": "~11.5",
    "one-svg-to-many-sized-pngs": "github:matatk/one-svg-to-many-sized-pngs#0.1.1",
    "puppeteer": "~1.0",
    "replace-in-file": "~3.1",
    "rimraf": "~2.6",
    "test": "~0.6"
  }
}
